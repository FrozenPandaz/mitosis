{
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ],
        "accessToken": "YTlkNzc4YzMtNzMzYS00NGFmLWJiZjktZDc2NjljZjc2OTVjfHJlYWQtd3JpdGU="
      }
    }
  },
  "namedInputs": {
    "default": [
      "!{projectRoot}/dist",
      "!{projectRoot}/output",
      "!{projectRoot}/playwright-results.json"
    ],
    "e2eFiles": ["{projectRoot}/src/**/*"]
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "clean"
      ],
      "inputs": [
        "default",
        "^e2eFiles"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "test": {
      "outputs": [
        "{projectRoot}/coverage"
      ]
    },
    "e2e": {
      "dependsOn": ["build"],
      "inputs": [
        "default",
        "^e2eFiles"
      ]
    }
  },
  "defaultBase": "main"
}
