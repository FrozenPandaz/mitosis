{
  "name": "@builder.io/e2e-alpine",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn run check && vite build",
    "check": "echo 'TODO: Alpine.js check script.'",
    "dev": "vite",
    "serve": "vite preview",
    "mitosis": "mitosis build alpine --config ../e2e-app/mitosis.config.js",
    "e2e": "../../node_modules/.bin/playwright test --config ../e2e-app/playwright.config.ts --grep e2e-alpine"
  },
  "dependencies": {
    "@builder.io/e2e-app": "workspace:*"
  },
  "devDependencies": {
    "alpinejs": "^3.10.5",
    "rimraf": "^3.0.2",
    "vite": "^2.9.13",
    "vite-plugin-handlebars": "^1.6.0"
  },
  "packageManager": "yarn@3.2.3"
}
